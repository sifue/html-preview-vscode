{
    "name": "UjZW2R7M",
    "displayName": "UjZW2R7M",
    "description": "A HTML previewer with some fix (Delete MathJax)",
    "version": "0.1.3",
    "publisher": "sifue",
    "author": "sifue",
    "license": "MIT",
    "readme": "README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/sifue/html-preview-vscode"
    },
    "galleryBanner": {
        "color": "#78d6f0",
        "theme": "light"
    },
    "bugs": "https://github.com/sifue/html-preview-vscode/issues",
    "homepage": "https://github.com/sifue/html-preview-vscode/blob/master/README.md",
    "icon": "media/icon.png",
    "main": "./out/src/extension",
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:html.preview",
        "onCommand:html.previewToSide",
        "onCommand:html.source"
    ],
    "contributes": {
        "keybindings": [{
                "command": "html.preview",
                "key": "ctrl+shift+h",
                "mac": "cmd+shift+h",
                "when": "!terminalFocus"
            },
            {
                "command": "html.previewToSide",
                "key": "ctrl+k h",
                "mac": "cmd+k h"
            }
        ],
        "commands": [{
                "command": "html.preview",
                "title": "Open Preview",
                "category": "HTML",
                "icon": {
                    "light": "./media/Preview.svg",
                    "dark": "./media/Preview_inverse.svg"
                }
            },
            {
                "command": "html.previewToSide",
                "title": "Open Preview to the Side",
                "category": "HTML",
                "icon": {
                    "light": "./media/PreviewOnRightPane_16x.svg",
                    "dark": "./media/PreviewOnRightPane_16x_dark.svg"
                }
            },
            {
                "command": "html.source",
                "title": "Show Source",
                "category": "HTML",
                "icon": {
                    "light": "./media/ViewSource.svg",
                    "dark": "./media/ViewSource_inverse.svg"
                }
            }
        ],
        "menus": {
            "editor/title": [{
                    "when": "editorLangId == html",
                    "command": "html.preview",
                    "alt": "html.previewToSide",
                    "group": "navigation"
                },
                {
                    "when": "resourceScheme == html",
                    "command": "html.source",
                    "group": "navigation"
                }
            ],
            "explorer/context": [{
                "when": "editorLangId == html",
                "command": "html.preview",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "build": "tsc -p ./",
        "vscode:prepublish": "npm run build",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "preversion": "npm run build",
        "version": "changes",
        "postversion": "git push --follow-tags"
    },
    "dependencies": {
        "file-url": "^2.0.0",
        "node-uuid": "^1.4.7"
    },
    "devDependencies": {
        "@studio/changes": "^1.0.4",
        "@types/file-url": "^1.0.28",
        "@types/node": "^6.0.52",
        "@types/node-uuid": "0.0.28",
        "typescript": "^2.1.4",
        "vscode": "^1.0.3"
    }
}